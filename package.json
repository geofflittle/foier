        "@aws-cdk/aws-logs": "^1.54.0",
        "@aws-cdk/aws-secretsmanager": "^1.54.0",
        "@aws-cdk/aws-sns": "^1.54.0",
        "@aws-cdk/core": "^1.54.0",
        "@types/aws-lambda": "^8.10.57",
        "@types/aws4": "^1.5.1",
